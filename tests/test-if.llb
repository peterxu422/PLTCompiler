void main(){
	// this is a test
    /*
     * Test Case:       if(true){}
     *
     * Result:          1
     */

    if (true) {
        print(1);
    }
    
    /*
     * Test Case:       if(false){}
     *
     * Result:          
     */

    if (false) {
        print(2);
    }

    /*
     * Test Case:       if(true){} else{}
     *
     * Result:          3
     */

    if (true) {
        print(3);
    }
    else {
        print(4);
    }

    /*
     * Test Case:       if(false){} else{}
     *
     * Result:          6
     */

    if (false) {
        print(5);
    }
    else {
        print(6);
    }
}
