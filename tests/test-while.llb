void main() {
    int i;
    boolean b;
        
    i = 0;
    while(i < 5) {
        print(i);
        i = i + 1;
    }
    print(42);
    
    b = true;
    while(b) {
        if(i == 10) {
            b = false;
        }
        print(i);
        i = i + 1;
    }
}
